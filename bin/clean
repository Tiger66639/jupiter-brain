#!/bin/bash

set -e

TOP_GOPATH=${GOPATH%%:*}
GO=${GO:-go}
PACKAGE=${PACKAGE:-github.com/travis-ci/jupiter-brain}
FIND=${FIND:-find}

rm -vf "${TOP_GOPATH}/bin/jb-server"
if [[ -d "${TOP_GOPATH}/pkg" ]]; then
	$FIND "${TOP_GOPATH}/pkg" -wholename \
		'*travis-ci/jupiter-brain*' | xargs rm -rfv || true
fi
